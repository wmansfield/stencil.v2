<?xml version="1.0" encoding="UTF-8"?>
<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    x:Class="Stencil.Native.Views.Standard.v1_0.H1">
    <DataTemplate x:Key="h1">
        <Grid RowSpacing="0"
              ColumnSpacing="0"
              Margin="0"
              Padding="0">
            <Label Text="{Binding PreparedData.Text}"
                   FormattedText="{Binding PreparedData.FormattedText}"
                   TextColor="White"
                   HorizontalTextAlignment="Center">
            </Label>
        </Grid>
    </DataTemplate>
</ResourceDictionary>