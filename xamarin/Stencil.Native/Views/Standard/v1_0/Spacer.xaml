<?xml version="1.0" encoding="UTF-8"?>
<ResourceDictionary xmlns="http://xamarin.com/schemas/2014/forms"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:local="clr-namespace:Stencil.Native.Views.Standard.v1_0"
                    x:Class="Stencil.Native.Views.Standard.v1_0.Spacer">
    <DataTemplate x:Key="spacer" x:DataType="local:SpacerContext">
        <!--<ViewCell>-->
            <AbsoluteLayout HeightRequest="{Binding Height}" 
                            BackgroundColor="{Binding BackgroundColor}" 
                            Padding="0"
                            Margin="0"
                            IsClippedToBounds="True">
            </AbsoluteLayout>
        <!--</ViewCell>-->
    </DataTemplate>
</ResourceDictionary>