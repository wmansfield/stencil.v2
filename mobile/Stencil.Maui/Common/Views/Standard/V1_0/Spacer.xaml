<?xml version="1.0" encoding="UTF-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:local="clr-namespace:Stencil.Maui.Views.Standard.v1_0"
                    x:Class="Stencil.Maui.Views.Standard.v1_0.Spacer">
    <DataTemplate x:Key="spacer" x:DataType="local:SpacerContext">
        <!--<ViewCell>-->
            <AbsoluteLayout HeightRequest="{Binding Height}" 
                            BackgroundColor="{Binding BackgroundColor, Converter={StaticResource stringToColorConverter}}" 
                            Padding="0"
                            Margin="0"
                            IsClippedToBounds="True">
            </AbsoluteLayout>
        <!--</ViewCell>-->
    </DataTemplate>
</ResourceDictionary>